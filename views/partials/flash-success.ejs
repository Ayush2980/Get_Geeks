<% if(success && success.length){ %>
<div class="alert alert-success d-flex align-items-center" style="height: 100px;" role="alert">
  <div class="row" style="width: 100%;">
    <div class="col-4">
      <%= success%>
    </div>
    <div class="col-7"></div>
    <div class="col-1">
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
  </div>
</div>
<%}%>